#ifdef APSTUDIO_INVOKED
#endif

#include <winres.h>
#include "../../include/svn_version.h"

#ifndef _DEBUG
# ifdef SVN_VER_NAME
#  define SVN_FILEFLAGS VS_FF_PRERELEASE
# else
#  define SVN_FILEFLAGS 0x0L
# endif
#else  /* _DEBUG */
# ifdef SVN_VER_NAME
#  define SVN_FILEFLAGS VS_FF_DEBUG|VS_FF_PRERELEASE
# else
#  define FILEFLAGS VS_FF_DEBUG
# endif
#endif /* _DEBUG */


1 ICON "svn.ico"

VS_VERSION_INFO VERSIONINFO
  FILEVERSION SVN_VER_MAJOR,SVN_VER_MINOR,SVN_VER_MICRO,0
  PRODUCTVERSION SVN_VER_MAJOR,SVN_VER_MINOR,SVN_VER_MICRO,0
  FILEFLAGSMASK VS_FFI_FILEFLAGSMASK
  FILEFLAGS SVN_FILEFLAGS
  FILEOS VOS__WINDOWS32
  FILETYPE VFT_APP
  FILESUBTYPE VFT2_UNKNOWN
BEGIN
  BLOCK "StringFileInfo"
  BEGIN
    BLOCK "040904B0"
    BEGIN
      VALUE "CompanyName", "http://subversion.tigris.org/\0"
       VALUE "FileDescription", SVN_VER_STRINGIFY(SVN_FILE_DESCRIPTION) "\0"
       VALUE "FileVersion", SVN_VER_NUMBER "\0"
       VALUE "InternalName", "SVN\0"
       VALUE "LegalCopyright", "Copyright 2002 CollabNet\0"
       VALUE "OriginalFilename", SVN_VER_STRINGIFY(SVN_FILE_NAME) "\0"
       VALUE "ProductName", "Subversion\0"
       VALUE "ProductVersion", SVN_VERSION "\0"
     END
   END
   BLOCK "VarFileInfo"
   BEGIN
     VALUE "Translation", 0x409, 1200
  END
END

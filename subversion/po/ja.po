msgstr "他の出力と連結するのに適した形式で出力を表示します"

# svn --encoding
#: clients/cmdline/main.c:92
msgid "treat value as being in charset encoding ARG"
msgstr "値が文字エンコーディング ARG であるとして処理します"

# svn --version
#: clients/cmdline/main.c:93
msgid "print client version info"
msgstr "クライアントのバージョン情報を表示します"

# svn --verbose
#: clients/cmdline/main.c:94
msgid "print extra information"
msgstr "付加的な情報を表示します"

# svn --show-updates
#: clients/cmdline/main.c:95
msgid "display update information"
msgstr "更新情報を表示します"

# svn --username
#: clients/cmdline/main.c:97
